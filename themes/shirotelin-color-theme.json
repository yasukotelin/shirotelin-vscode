{
	"name": "shirotelin",
	"type": "light",
	"colors": {
		"foreground": "#000000",
		"badge.background": "#000080",
		"badge.foreground": "#FFFFFF",
		"editor.background": "#FFFFFF",
		"editor.foreground": "#000000",
		"editor.selectionBackground": "#CDE8FF",
		"editor.hoverHighlightBackground": "#EEEEEE",
		"editor.findMatchBackground": "#cfcca3",
		"editor.findMatchHighlightBackground": "#ffff80",
		"editorSuggestWidget.background": "#E4E4E4",
		"editorSuggestWidget.foreground": "#000000",
		"editorSuggestWidget.highlightForeground": "#000000",
		"editorSuggestWidget.selectedBackground": "#CDE8FF",
		"editorBracketMatch.background": "#ffc7fe",
		"sideBar.background": "#FFFFFF",
		"sideBar.border": "#CFCFCF",
		"titleBar.activeBackground": "#FFFFFF",
		"titleBar.activeForeground": "#000000",
		"titleBar.inactiveBackground": "#FFFFFF",
		"titleBar.inactiveForeground": "#000000",
		"titleBar.border": "#CFCFCF",
		"statusBar.background": "#000080",
		"statusBar.foreground": "#FFFFFF",
		"editorCursor.background": "#FFFFFF",
		"editorCursor.foreground": "#000000",
		"diffEditor.insertedTextBackground": "#C8FFC880",
		"diffEditor.removedTextBackground": "#FFC8C880",
		"editorLineNumber.foreground": "#545454",
		"editorLineNumber.activeForeground": "#005f5f",
		"terminal.background": "#FFFFFF",
		"terminal.foreground": "#000000",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiWhite": "#FFFFFF",
		"terminal.ansiBrightBlack": "#000000",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBrightRed": "#FF0000",
		"terminal.ansiRed": "#FF0000",
		"terminal.ansiBrightYellow": "#7A7946",
		"terminal.ansiYellow": "#7A7946",
		"terminal.ansiBrightBlue": "#0000AF",
		"terminal.ansiBlue": "#0000AF",
		"terminal.ansiBrightCyan": "#005F5F",
		"terminal.ansiCyan": "#005F5F",
		"terminal.ansiBrightGreen": "#005F00",
		"terminal.ansiGreen": "#005F00",
		"terminal.ansiBrightMagenta": "#AF00AF",
		"terminal.ansiMagenta": "#AF00AF"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#005F00"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#005F00"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#005F00"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#87005f"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#87005f"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#87005f"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#00005F"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#00005F"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#00005F"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#005f5f"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#0000AF"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#0000AF"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#AF00AF"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#AF00AF"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#0000AF"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				// "fontStyle": "bold",
				"foreground": "#00005F"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#87005f"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#875f87"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#87005f"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#87005f"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#af00af"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#87005f"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#87005f"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#00005F"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
